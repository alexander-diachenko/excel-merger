                        </HBox>
                        <Button fx:id="format" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="140.0" text="Format" GridPane.rowIndex="5">
                            <opaqueInsets>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </opaqueInsets>
                        </Button>
                        <Label fx:id="complete" prefHeight="96.0" prefWidth="507.0" GridPane.rowIndex="6" />
                        <ProgressIndicator fx:id="progressIndicator" prefHeight="92.0" prefWidth="147.0" progress="0.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                    </children>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <padding>
